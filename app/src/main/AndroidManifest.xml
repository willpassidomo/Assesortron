<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="test.assesortron3" >

    <uses-feature android:name="android.hardware.camera" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".UserSelect"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HomeScreen"
            android:label="@string/title_activity_home_screen"
            android:launchMode="singleTop"
            android:parentActivityName=".UserSelect" >
        </activity>
        <activity
            android:name=".ProjectHomeScreen"
            android:label="@string/title_activity_project_home_screen"
            android:launchMode="singleTop"
            android:parentActivityName=".HomeScreen" >
        </activity>
        <activity
            android:name=".SiteWalkthrough"
            android:label="@string/title_activity_site_walkthrough"
            android:parentActivityName=".VisitSite" >
        </activity>
        <activity
            android:name=".MakeDrawG702"
            android:label="@string/title_activity_make_draw"
            android:parentActivityName=".MakeDraw" >
        </activity>
        <activity
            android:name=".ProjectDashboard"
            android:label="@string/title_activity_project_dashboard"
            android:parentActivityName=".ProjectHomeScreen" >
        </activity>
        <activity
            android:name=".History"
            android:label="@string/title_activity_history"
            android:parentActivityName=".ProjectHomeScreen" >
        </activity>
        <activity
            android:name=".StringListEdit"
            android:label="@string/title_activity_string_list_edit"
            android:parentActivityName=".ProjectHomeScreen" >
        </activity>
        <activity
            android:name=".SendProject"
            android:label="@string/title_activity_send_project"
            android:parentActivityName=".ProjectHomeScreen" >
        </activity>
        <activity
            android:name=".MakeDraw"
            android:label="@string/title_activity_make_draw"
            android:parentActivityName=".VisitSite" >
        </activity>
        <activity
            android:name=".VisitSite"
            android:label="@string/title_activity_site_visit"
            android:launchMode="singleTop"
            android:parentActivityName=".ProjectHomeScreen" >
        </activity>
        <activity
            android:name=".NewSiteVisit"
            android:label="@string/title_activity_new_site_visit" >
        </activity>

        <service
            android:name="test.Network.FullSyncService"
            android:exported="false" >
        </service>
    </application>

</manifest>
