<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="test.assesortron3"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="21" />

    <uses-feature android:name="android.hardware.camera" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="test.assesortron3.UserSelect"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="test.assesortron3.HomeScreen"
            android:label="@string/title_activity_home_screen"
            android:launchMode="singleTop"
            android:parentActivityName="test.assesortron3.UserSelect" >
        </activity>
        <activity
            android:name="test.assesortron3.ProjectHomeScreen"
            android:label="@string/title_activity_project_home_screen"
            android:launchMode="singleTop"
            android:parentActivityName="test.assesortron3.HomeScreen" >
        </activity>
        <activity
            android:name="test.assesortron3.SiteWalkthrough"
            android:label="@string/title_activity_site_walkthrough"
            android:parentActivityName="test.assesortron3.VisitSite" >
        </activity>
        <activity
            android:name="test.assesortron3.MakeDrawG702"
            android:label="@string/title_activity_make_draw"
            android:parentActivityName="test.assesortron3.MakeDraw" >
        </activity>
        <activity
            android:name="test.assesortron3.ProjectDashboard"
            android:label="@string/title_activity_project_dashboard"
            android:parentActivityName="test.assesortron3.ProjectHomeScreen" >
        </activity>
        <activity
            android:name="test.assesortron3.History"
            android:label="@string/title_activity_history"
            android:parentActivityName="test.assesortron3.ProjectHomeScreen" >
        </activity>
        <activity
            android:name="test.assesortron3.StringListEdit"
            android:label="@string/title_activity_string_list_edit"
            android:parentActivityName="test.assesortron3.ProjectHomeScreen" >
        </activity>
        <activity
            android:name="test.assesortron3.SendProject"
            android:label="@string/title_activity_send_project"
            android:parentActivityName="test.assesortron3.ProjectHomeScreen" >
        </activity>
        <activity
            android:name="test.assesortron3.MakeDraw"
            android:label="@string/title_activity_make_draw"
            android:parentActivityName="test.assesortron3.VisitSite" >
        </activity>
        <activity
            android:name="test.assesortron3.VisitSite"
            android:label="@string/title_activity_site_visit"
            android:launchMode="singleTop"
            android:parentActivityName="test.assesortron3.ProjectHomeScreen" >
        </activity>

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>